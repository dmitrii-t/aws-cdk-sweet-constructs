{
  "name": "aws-cdk-sweet-constructs",
  "version": "0.0.1",
  "description": "aws-cdk-sweet-constructs",
  "main": "dist/stack.js",
  "license": "MIT",
  "engines": {
    "node": ">=10.x"
  },
  "scripts": {
    "aws": "TS_NODE_PROJECT=tsconfig.scripts.json env-cmd node -e \"require(\\\"./babel-hook\\\"); require(\\\"./scripts/aws\\\").aws()\"",
    "bld": "TS_NODE_PROJECT=tsconfig.bundle.json NODE_ENV=production env-cmd webpack -p",
    "cln": "rimraf dist",
    "lnt": "tslint -c tslint.json 'src/**/*.ts' 'lib/**/*.ts'",
    "usr": "npm run aws iam:usr",
    "stk": "TS_NODE_PROJECT=tsconfig.scripts.json env-cmd node ./scripts/stack-hook",
    "tst": "TS_NODE_PROJECT=tsconfig.test.json env-cmd mocha --timeout 600000 --require ./babel-hook.js \"./lib/**/*.spec.ts\"",
    "typ": "tsc",
    "wch": "TS_NODE_PROJECT=tsconfig.bundle.json env-cmd webpack --watch --progress"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 120
  },
  "keywords": [
    "typescript",
    "starter-kit",
    "webpack",
    "node",
    "aws"
  ],
